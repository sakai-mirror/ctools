+	private String readIntoString(ZipInputStream zin) throws IOException {
+		byte[] buf = new byte[1024];
+		int len;
+		StringBuffer b = new StringBuffer();
+		while ((len = zin.read(buf)) > 0) {
+		    b.append(new String(buf));
+		}
+		return b.toString();
+	}
