+				<input name="gradeFile" type="checkbox" value="gradeFile" id="gradeFile" #if($!hasGradeFile)checked="checked"#end />
+				<label for="gradeFile">$tlang.getString("uploadall.choose.file.gradeFile")</label><br/>
